{
  "compilerOptions": {
    "noImplicitAny": false,
    "outDir": "./dist",
    "allowJs": true,
    "target": "ESNext",
    "module": "commonjs",
    "jsx": "react-native",
    "lib": ["ES6", "DOM"],
    "checkJs": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "baseUrl": "."
  }
}
